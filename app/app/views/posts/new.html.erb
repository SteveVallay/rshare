<h1>new share </h1>

<%= form_for :post do |f| %>
  <p>
  <%= f.label  :title %></br>
  <%= f.text_field :title %>
  </p>

  <p>
  <%= f.label :text %> </br>
  <%= f.text_area :text %>
  </p>

  <p>
  <%= f.label :name %></br>
  <%= f.file_field :name %>
  </p>

  <p>
    <%= f.submit %>
  </p>

  <% end %>


